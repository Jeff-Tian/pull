{
  "name": "pull",
  "version": "0.0.0-development",
  "description": "k8s switcher",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "bin": "lib/index.js",
  "scripts": {
    "start": "ts-node src/index.ts --version",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint --fix -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "postinstall": "echo 'Thanks for using pull!'",
    "semantic-release": "semantic-release",
    "i:taobao": "npm i --registry=https://registry.npm.taobao.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jeff-Tian/pull.git"
  },
  "keywords": [
    "download",
    "pull"
  ],
  "author": "jeff.tian@outlook.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jeff-Tian/pull/issues"
  },
  "homepage": "https://github.com/Jeff-Tian/pull#readme",
  "dependencies": {
    "file-name-expert": "^1.0.0",
    "request": "^2.88.2",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "yargs": "^13.2.4",
    "ramda": "^0.27.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^8.0.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/ramda": "^0.26.41",
    "@types/request": "^2.48.4",
    "@types/rimraf": "^2.0.2",
    "@types/shelljs": "^0.8.5",
    "@types/sinon": "^7.5.1",
    "@types/yargs": "^13.0.0",
    "coffee": "^5.2.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "semantic-release": "^17.0.3",
    "sinon": "^8.1.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.6.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.4.5"
  }
}
